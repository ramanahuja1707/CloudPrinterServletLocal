<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>ObjectifyRegisterService</servlet-name>
		<servlet-class>com.cloudprinter.startup.ObjectifyRegisterService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ObjectifyRegisterService</servlet-name>
		<url-pattern>/objectifystart</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.cloudprinter.servlet.Login</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>Logout</servlet-name>
		<servlet-class>com.cloudprinter.servlet.Logout</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>ForgotLoginId</servlet-name>
		<servlet-class>com.cloudprinter.servlet.ForgotLoginId</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ForgotLoginId</servlet-name>
		<url-pattern>/forgotloginid</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>ForgotPassword</servlet-name>
		<servlet-class>com.cloudprinter.servlet.ForgotPassword</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ForgotPassword</servlet-name>
		<url-pattern>/forgotpassword</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>RegisterUser</servlet-name>
		<servlet-class>com.cloudprinter.servlet.RegisterUser</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterUser</servlet-name>
		<url-pattern>/registeruser</url-pattern>
	</servlet-mapping>


	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
